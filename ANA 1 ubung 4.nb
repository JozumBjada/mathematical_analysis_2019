Cell[TextData[{
 StyleBox["Satz 4.1.",
  FontWeight->"Bold"],
 ": Ist ",
 Cell[BoxData[
  FormBox[
   RowBox[{"p", "(", "z", ")"}], TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "e513c19f-4f8e-456a-b127-1d4e52c6dc71"],
 " ein Polynom vom Grad ",
 Cell[BoxData[
  FormBox[
   RowBox[{"n", "\[Element]", "\[DoubleStruckCapitalN]"}], TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "be2662c6-0b49-4a56-b800-8906e228168f"],
 " und ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["z", "0"], TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "579d4722-b36d-42b3-bb83-b0d1165fbdf6"],
 " eine Nullstelle von ",
 Cell[BoxData[
  FormBox["p", TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "fb2573b0-67cd-4bf6-b61a-724e26ef041c"],
 ", also ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"p", "(", 
     SubscriptBox["z", "0"], ")"}], "=", "0"}], TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "8ed613d5-f370-49f6-83c2-68d0af1eeb44"],
 ", dann gibt es ein Polynom ",
 Cell[BoxData[
  FormBox[
   RowBox[{"q", "(", "z", ")"}], TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "1a6dafa3-71c3-4204-bd2b-f4a6a09c7044"],
 " vom Grad ",
 Cell[BoxData[
  FormBox[
   RowBox[{"n", "-", "1"}], TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "c4058081-6f2d-4a57-b82e-e09b66bcb549"],
 " mit ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"p", "(", "z", ")"}], "=", 
    RowBox[{
     RowBox[{"(", 
      RowBox[{"z", "-", 
       SubscriptBox["z", "0"]}], ")"}], 
     RowBox[{"q", "(", "z", ")"}]}]}], TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "87e0f834-709f-4d8e-821c-eec91eb1217f"],
 "."
}], "Text",
 CellChangeTimes->{{3.781016635089367*^9, 3.7810166686736803`*^9}, {
