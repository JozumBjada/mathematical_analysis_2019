Cell[TextData[{
 "Since ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    FractionBox[
     SubscriptBox["a", 
      RowBox[{"n", "+", "1"}]], 
     SubscriptBox["a", "n"]], "=", 
    RowBox[{
     FractionBox[
      SuperscriptBox["x", 
       RowBox[{"2", 
        RowBox[{"(", 
         RowBox[{"n", "+", "1"}], ")"}]}]], 
      SuperscriptBox["x", 
       RowBox[{"2", "n"}]]], "=", 
     RowBox[{
      SuperscriptBox["x", "2"], "<", "1"}]}]}], TraditionalForm]],
  ExpressionUUID->"c16b69e2-a257-45da-ba13-b0aa5623f4ff"],
 ", we get ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["a", 
     RowBox[{"n", "+", "1"}]], "<", 
    SubscriptBox["a", "n"]}], TraditionalForm]],ExpressionUUID->
  "8cff3a6e-2206-40a4-8466-be51fec9dd65"],
 ". The sequence is therefore monotonically decreasing."
}], "Text",
 CellChangeTimes->{{3.781017460671653*^9, 3.781017470422267*^9}, {
  3.7810185207685533`*^9, 3.7810187159620857`*^9}, {3.7810204707525854`*^9, 
  3.7810204723553047`*^9}, {3.781080697428115*^9, 3.7810807415410643`*^9}},
 FontSize->24,ExpressionUUID->"d8bb6d81-ca56-48c8-9ffa-fb30db4702c2"],

Cell[TextData[{
 "As ",
 Cell[BoxData[
  FormBox[
   RowBox[{"0", "<", "x", "<", "1"}], TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "9f81154e-1aca-4c9f-bf8e-b5f0a4f6efe5"],
 ", we have ",
 Cell[BoxData[
  FormBox[
   RowBox[{"0", "<", 
    SuperscriptBox["x", 
     RowBox[{"2", "n"}]], "<", "1"}], TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "3e5d8473-7b40-4969-9383-452ebfe34660"],
 " and ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["a", "n"], TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "81d7d8cb-8317-4214-baad-a6f5f97fed06"],
 " is bounded. Therefore, ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["a", "n"], TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "c36affa2-aa0a-42b3-a199-03143b637da9"],
 " is convergent."
}], "Text",
 CellChangeTimes->{{3.7810807645444603`*^9, 3.781080814307494*^9}},
 FontSize->24,ExpressionUUID->"319e97c6-a027-4343-8c2a-c7f8d38cbf0a"],

