Cell[CellGroupData[{

Cell["Solution", "Chapter",
 CellChangeTimes->{{3.781016572580119*^9, 
  3.781016573215418*^9}},ExpressionUUID->"c7c5d5a2-8311-4320-8322-\
522b893bd681"],

Cell[TextData[{
 "Sequence ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["a", "n"], TraditionalForm]],ExpressionUUID->
  "677ccc6b-ff6c-43eb-91ca-650dc657d48e"],
 " is bounded and monotonic \[Rule] limit (Grenzwert) exists."
}], "Text",
 CellChangeTimes->{{3.7810764323024073`*^9, 3.7810764776371975`*^9}, {
  3.7815087314272985`*^9, 3.781508734660578*^9}},
 FontSize->24,ExpressionUUID->"fc8bfb0a-feac-4f84-b5e8-438eed29db6e"],

Cell[TextData[Cell[BoxData[
 FormBox[
  RowBox[{
   RowBox[{
    SubscriptBox["\[Limit]", 
     RowBox[{"n", "\[Rule]", "\[Infinity]"}]], 
    SubscriptBox["a", "n"]}], "=", 
   RowBox[{
    RowBox[{
     SubscriptBox["\[Limit]", 
      RowBox[{"n", "\[Rule]", "\[Infinity]"}]], 
     FractionBox[
      RowBox[{
       SuperscriptBox["n", "2"], "-", "n"}], 
      RowBox[{
       RowBox[{"2", 
        SuperscriptBox["n", "2"]}], "+", "3"}]]}], "=", 
    RowBox[{
     RowBox[{
      SubscriptBox["\[Limit]", 
       RowBox[{"n", "\[Rule]", "\[Infinity]"}]], 
      FractionBox[
       RowBox[{
        SuperscriptBox["n", "2"], "(", 
        RowBox[{"1", "-", 
         FractionBox["1", "n"]}], ")"}], 
       RowBox[{
        SuperscriptBox["n", "2"], "(", 
        RowBox[{"2", "+", 
         FractionBox["3", 
          SuperscriptBox["n", "2"]]}], ")"}]]}], "=", 
     RowBox[{
      RowBox[{
       SubscriptBox["\[Limit]", 
        RowBox[{"n", "\[Rule]", "\[Infinity]"}]], 
       FractionBox[
        RowBox[{"1", "-", 
         FractionBox["1", "n"]}], 
        RowBox[{"2", "+", 
         FractionBox["3", 
          SuperscriptBox["n", "2"]]}]]}], "=", 
      FractionBox["1", "2"]}]}]}]}], 
  TraditionalForm]],ExpressionUUID->"4422e727-f798-4a55-ae9b-8f963f0b9af2"]], \
"Text",
 CellChangeTimes->{{3.7810765360079517`*^9, 3.781076618775659*^9}},
 FontSize->24,ExpressionUUID->"c5293032-f949-415e-bae7-f395257e99bc"],

Cell[TextData[{
 "Or by definition: At first, we see in the plot above, that ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["a", "n"], TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "0f3bea99-ccd7-4dca-9a3f-100999a08041"],
 " approaches ",
 Cell[BoxData[
  FormBox[
   FractionBox["1", "2"], TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "37aa8641-1204-42fe-92cd-fd6351954370"],
 " for large ",
 Cell[BoxData[
  FormBox["n", TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "a3ec6fff-a2c8-40fa-808c-80852de0fdca"],
 ". We then guess that the limit is indeed ",
 Cell[BoxData[
  FormBox[
   FractionBox["1", "2"], TraditionalForm]],ExpressionUUID->
  "bdee0a72-2795-4f4d-9375-625ea98190fb"],
 " and use the definition of the limit to really prove it is so."
}], "Text",
 CellChangeTimes->{{3.7810766261649027`*^9, 3.781076749202419*^9}},
 FontSize->24,ExpressionUUID->"310f8923-ea88-4715-abcc-74a0e1163e1d"],

Cell[TextData[{
 "We want to show, that: ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"(", 
     RowBox[{"\[ForAll]", 
      RowBox[{"\[Epsilon]", ">", "0"}]}], ")"}], 
    RowBox[{"(", 
     RowBox[{"\[Exists]", 
      RowBox[{
       SubscriptBox["n", "0"], "\[Element]", "\[DoubleStruckCapitalN]"}]}], 
     ")"}], 
    RowBox[{"(", 
     RowBox[{
      RowBox[{"\[ForAll]", 
       RowBox[{"n", "\[Element]", "\[DoubleStruckCapitalN]"}]}], ",", " ", 
      RowBox[{"n", ">", 
       SubscriptBox["n", "0"]}]}], ")"}], 
    RowBox[{"(", 
     RowBox[{"|", 
      RowBox[{
       SubscriptBox["a", "n"], "-", 
       FractionBox["1", "2"]}], "|", 
      RowBox[{"<", "\[Epsilon]"}]}], ")"}]}], TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "f27175c5-4fa3-42e6-89a9-1ee85014dd86"]
}], "Text",
 CellChangeTimes->{{3.781076753583706*^9, 3.7810768567917643`*^9}},
 FontSize->24,ExpressionUUID->"818d980a-6512-4e05-915e-a3c30cfc8255"],

Cell[TextData[Cell[BoxData[
 FormBox[
  RowBox[{
   RowBox[{"|", 
    RowBox[{
     SubscriptBox["a", "n"], "-", 
     FractionBox["1", "2"]}], "|"}], "=", 
   RowBox[{
    RowBox[{"|", 
     RowBox[{
      FractionBox[
       RowBox[{
        SuperscriptBox["n", "2"], "-", "n"}], 
       RowBox[{
        RowBox[{"2", 
         SuperscriptBox["n", "2"]}], "+", "3"}]], "-", 
      FractionBox["1", "2"]}], "|"}], "=", 
    RowBox[{
     RowBox[{"|", 
      FractionBox[
       RowBox[{
        RowBox[{
         RowBox[{"-", "2"}], " ", "n"}], "-", "3"}], 
       RowBox[{"2", " ", 
        RowBox[{"(", 
         RowBox[{
          RowBox[{"2", " ", 
           SuperscriptBox["n", "2"]}], "+", "3"}], ")"}]}]], "|"}], "=", 
     RowBox[{
      FractionBox[
       RowBox[{
        RowBox[{"2", " ", "n"}], "+", "3"}], 
       RowBox[{"2", " ", 
        RowBox[{"(", 
         RowBox[{
          RowBox[{"2", " ", 
           SuperscriptBox["n", "2"]}], "+", "3"}], ")"}]}]], "<", 
      "\[Epsilon]"}]}]}]}], TraditionalForm]],
 FormatType->
  "TraditionalForm",ExpressionUUID->"68ad1b70-0cdc-4f8e-b59d-d6f53d76dced"]], \
"Text",
 CellChangeTimes->{{3.7810768694080305`*^9, 3.7810769950720468`*^9}},
 FontSize->24,ExpressionUUID->"dc956563-22af-4a9f-95c8-60b3696ec26c"],

Cell[TextData[{
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     FractionBox[
      RowBox[{
       RowBox[{"2", " ", "n"}], "+", "3"}], 
      RowBox[{"2", " ", 
       RowBox[{"(", 
        RowBox[{
         RowBox[{"2", " ", 
          SuperscriptBox["n", "2"]}], "+", "3"}], ")"}]}]], "<", 
     "\[Epsilon]"}], "\[DoubleLeftRightArrow]", 
    RowBox[{
     RowBox[{
      RowBox[{"2", " ", "n"}], "+", "3"}], "<", 
     RowBox[{"2", "\[Epsilon]", " ", 
      RowBox[{"(", 
       RowBox[{
        RowBox[{"2", " ", 
         SuperscriptBox["n", "2"]}], "+", "3"}], ")"}]}]}], 
    "\[DoubleLeftRightArrow]", 
    RowBox[{"0", "<", 
     RowBox[{
      RowBox[{"4", "\[Epsilon]", " ", 
       SuperscriptBox["n", "2"]}], "-", 
      RowBox[{"2", "n"}], "+", 
      RowBox[{"6", "\[Epsilon]"}], "-", "3"}]}]}], TraditionalForm]],
  ExpressionUUID->"4d099d21-73fb-4b0e-9475-dbe78fe874c9"],
 "   ...(\[Star])"
}], "Text",
 CellChangeTimes->{{3.781077033925738*^9, 3.7810771091392264`*^9}, {
  3.781508935736356*^9, 3.7815089660356307`*^9}},
 FontSize->24,ExpressionUUID->"12ce03e4-7f3d-4b52-9a25-e5f78ea9d3da"],

Cell[TextData[{
 "There are many ways how we can determine ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["n", "0"], TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "9df612df-1347-4547-9f73-0f34ef847624"],
 ", one of them is to directly solve the formula above:"
}], "Text",
 CellChangeTimes->{{3.7815088752295074`*^9, 3.781508923104524*^9}},
 FontSize->24,ExpressionUUID->"e54794d1-21be-47ea-a6d0-fa6e7a470c80"],

Cell[TextData[{
 "Roots of the quadratic function on the RHS of (\[Star]) (general formula ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["n", 
     RowBox[{"1", ",", "2"}]], "=", 
    FractionBox[
     RowBox[{
      RowBox[{"-", "b"}], "\[PlusMinus]", 
      SqrtBox[
       RowBox[{
        SuperscriptBox["b", "2"], "-", 
        RowBox[{"4", " ", "a", " ", "c"}]}]]}], 
     RowBox[{"2", "a"}]]}], TraditionalForm]],ExpressionUUID->
  "8fb43309-84c5-401f-a17e-4a0401a89bfe"],
 "): ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["n", 
     RowBox[{"1", ",", "2"}]], "=", 
    RowBox[{
     FractionBox[
      RowBox[{"2", "\[PlusMinus]", 
       SqrtBox[
        RowBox[{"4", "-", 
         RowBox[{"4", " ", "4", "\[Epsilon]", " ", 
          RowBox[{"(", 
           RowBox[{
            RowBox[{"6", "\[Epsilon]"}], "-", "3"}], ")"}]}]}]]}], 
      RowBox[{"8", " ", "\[Epsilon]"}]], "=", 
     FractionBox[
      RowBox[{"1", "\[PlusMinus]", 
       SqrtBox[
        RowBox[{"1", "-", 
         RowBox[{"24", 
          SuperscriptBox["\[Epsilon]", "2"]}], " ", "+", 
         RowBox[{"12", "\[Epsilon]"}]}]]}], 
      RowBox[{"4", " ", "\[Epsilon]"}]]}]}], TraditionalForm]],ExpressionUUID->
  "2afe1b3c-4965-41a1-aac7-89145ed18738"],
 "."
}], "Text",
 CellChangeTimes->{{3.7810771243611727`*^9, 3.781077134621353*^9}, {
   3.7810771671663*^9, 3.7810772850797725`*^9}, 3.781078047432214*^9, {
   3.78150895904749*^9, 3.7815089630807853`*^9}},
 FontSize->24,ExpressionUUID->"45b943ca-3963-4532-a883-d442db9150cc"],

Cell[TextData[{
 "So for given ",
 Cell[BoxData[
  FormBox["\[Epsilon]", TraditionalForm]],ExpressionUUID->
  "fbae2a6e-aacc-4ffb-97a0-37633b6004c4"],
 " we can set ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["n", "0"], ":=", 
    RowBox[{
     RowBox[{"\[LeftCeiling]", 
      FractionBox[
       RowBox[{"1", "+", 
        SqrtBox[
         RowBox[{"1", "-", 
          RowBox[{"24", 
           SuperscriptBox["\[Epsilon]", "2"]}], " ", "+", 
          RowBox[{"12", "\[Epsilon]"}]}]]}], 
       RowBox[{"4", " ", "\[Epsilon]"}]], "\[RightCeiling]"}], "\[Element]", 
     "\[DoubleStruckCapitalN]"}]}], TraditionalForm]],ExpressionUUID->
  "ec704b28-5fda-4fb9-9cc3-987468be0016"],
 "."
}], "Text",
 CellChangeTimes->{{3.781078053101596*^9, 3.7810781112335644`*^9}},
 FontSize->24,ExpressionUUID->"f68deb94-d205-402f-94ec-150fd2110967"],

Cell[TextData[{
 "Another possibility is for example  ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["n", "0"], ":=", 
    RowBox[{"\[LeftCeiling]", 
     FractionBox["1", "\[Epsilon]"], "\[RightCeiling]"}]}], TraditionalForm]],
  ExpressionUUID->"2bc3c78b-0d2f-44f9-98fd-cf682250de33"],
 "."
}], "Text",
 CellChangeTimes->{{3.7815091059540977`*^9, 3.7815091451163845`*^9}, {
  3.781511025512048*^9, 3.781511025973827*^9}, {3.7815112515176497`*^9, 
  3.7815112929019165`*^9}},
 FontSize->24,ExpressionUUID->"dfc496cb-3887-46b6-8c3a-db9e5980b062"]
}, Closed]],

