   RowBox[{
    RowBox[{
     SubscriptBox["\[Limit]", 
      RowBox[{"n", "\[Rule]", "\[Infinity]"}]], 
     SubscriptBox["a", "n"]}], "=", "0"}], TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "d79f6f3e-6916-43f8-9570-b6fd694da729"],
 " ?"
}], "Text",
