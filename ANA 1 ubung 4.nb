Cell[CellGroupData[{

Cell["Solution", "Chapter",
 CellChangeTimes->{{3.781016572580119*^9, 
  3.781016573215418*^9}},ExpressionUUID->"33d698a8-6023-48f9-8e0b-\
57efc7a07abc"],

Cell[TextData[{
 "Since ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     SubscriptBox["a", 
      RowBox[{"n", "+", "1"}]], "-", 
     SubscriptBox["a", "n"]}], "=", 
    RowBox[{
     RowBox[{
      FractionBox[
       RowBox[{
        SuperscriptBox[
         RowBox[{"(", 
          RowBox[{"n", "+", "1"}], ")"}], "2"], "-", 
        RowBox[{"(", 
         RowBox[{"n", "+", "1"}], ")"}]}], 
       RowBox[{
        RowBox[{"2", 
         SuperscriptBox[
          RowBox[{"(", 
           RowBox[{"n", "+", "1"}], ")"}], "2"]}], "+", "3"}]], "-", 
      FractionBox[
       RowBox[{
        SuperscriptBox["n", "2"], "-", "n"}], 
       RowBox[{
        RowBox[{"2", 
         SuperscriptBox["n", "2"]}], "+", "3"}]]}], "=", 
     RowBox[{
      FractionBox[
       RowBox[{"2", " ", 
        RowBox[{"(", 
         RowBox[{
          RowBox[{"4", " ", "n"}], "+", 
          SuperscriptBox["n", "2"]}], ")"}]}], 
       RowBox[{
        RowBox[{"(", 
         RowBox[{"3", "+", 
          RowBox[{"2", " ", 
           SuperscriptBox["n", "2"]}]}], ")"}], " ", 
        RowBox[{"(", 
         RowBox[{"5", "+", 
          RowBox[{"4", " ", "n"}], "+", 
          RowBox[{"2", " ", 
           SuperscriptBox["n", "2"]}]}], ")"}]}]], ">", "0"}]}]}], 
   TraditionalForm]],ExpressionUUID->"59c6ce11-3a08-4840-9896-1da8cd7c378f"],
 ", we get ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["a", 
     RowBox[{"n", "+", "1"}]], ">", 
    SubscriptBox["a", "n"]}], TraditionalForm]],ExpressionUUID->
  "643f28a3-6b7e-448f-84e6-17b67e202df6"],
 ". The sequence is therefore monotonically increasing."
}], "Text",
 CellChangeTimes->{{3.781017460671653*^9, 3.781017470422267*^9}, {
  3.7810185207685533`*^9, 3.7810187159620857`*^9}, {3.7810204707525854`*^9, 
  3.7810204723553047`*^9}},
 FontSize->24,ExpressionUUID->"a0609f8a-0801-49b1-a0f0-fcf8d2aa198e"],

Cell[TextData[{
 "The sequence is bounded from below as ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["a", "n"], "=", 
    RowBox[{
     FractionBox[
      RowBox[{
       SuperscriptBox["n", "2"], "-", "n"}], 
      RowBox[{
       RowBox[{"2", 
        SuperscriptBox["n", "2"]}], "+", "3"}]], "=", 
     RowBox[{
      FractionBox[
       RowBox[{"n", "(", 
        RowBox[{"n", "-", "1"}], ")"}], 
       RowBox[{
        RowBox[{"2", 
         SuperscriptBox["n", "2"]}], "+", "3"}]], "\[GreaterEqual]", 
      "0"}]}]}], TraditionalForm]],ExpressionUUID->
  "b2781877-a9d9-4673-bd96-e1a2f0b89a0a"],
 "."
}], "Text",
 CellChangeTimes->{{3.781018865630781*^9, 3.7810188836896906`*^9}, {
  3.7810204076599245`*^9, 3.781020418562643*^9}, {3.7810204786120253`*^9, 
  3.7810204794482756`*^9}, {3.7814299433646555`*^9, 3.781429945055169*^9}},
 FontSize->24,ExpressionUUID->"19492215-9488-4b1f-9b75-965811df8229"],

Cell[TextData[{
 "Is the sequence bounded from above? Let\[CloseCurlyQuote]s say the bound is \
1. Is ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["a", "n"], "=", 
    RowBox[{
     FractionBox[
      RowBox[{
       SuperscriptBox["n", "2"], "-", "n"}], 
      RowBox[{
       RowBox[{"2", 
        SuperscriptBox["n", "2"]}], "+", "3"}]], "<", "1"}]}], 
   TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "ad2a0f00-501f-4163-8db4-8110a547eb11"],
 "?"
}], "Text",
 CellChangeTimes->{{3.7810189111429834`*^9, 3.7810189273686085`*^9}, {
  3.7810204271365643`*^9, 3.7810204653720036`*^9}},
 FontSize->24,ExpressionUUID->"8977ad84-958d-44e0-b082-c1272ee309e5"],

Cell[TextData[Cell[BoxData[
 FormBox[
  RowBox[{
   RowBox[{
    FractionBox[
     RowBox[{
      SuperscriptBox["n", "2"], "-", "n"}], 
     RowBox[{
      RowBox[{"2", 
       SuperscriptBox["n", "2"]}], "+", "3"}]], "<", "1"}], 
   "\[DoubleLeftRightArrow]", 
   RowBox[{
    RowBox[{
     SuperscriptBox["n", "2"], "-", "n"}], "<", 
    RowBox[{
     RowBox[{"2", 
      SuperscriptBox["n", "2"]}], "+", "3"}]}], "\[DoubleLeftRightArrow]", 
   RowBox[{"0", "<", 
    RowBox[{
     SuperscriptBox["n", "2"], "+", "n", "+", "3"}]}]}], 
  TraditionalForm]],ExpressionUUID->"d08a126f-bedb-4083-9b8e-1fc4410f91a0"]], \
"Text",
 CellChangeTimes->{{3.781018932560413*^9, 3.781018972628874*^9}},
 FontSize->24,ExpressionUUID->"a30d88a8-844a-4319-bfa7-3056b84003d6"],

Cell[TextData[{
 "Expression ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SuperscriptBox["n", "2"], "+", "n", "+", "3"}], TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "1e762f32-9290-4008-b0a0-7caab3dc1da5"],
 " is indeed always positive (there are no real solutions to ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     SuperscriptBox["n", "2"], "+", "n", "+", "3"}], "=", "0"}], 
   TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "e0271b6d-38d3-4dfb-b8f3-c3e6f6b5d8f6"],
 "). Therefore ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["a", "n"], "<", "1"}], TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "e64b476b-d0d6-4f2c-9d3d-6f6682a2f635"],
 " and the sequence ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["a", "n"], TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "c4e3e933-a588-45a0-983a-fdd22fe7f20d"],
 " is bounded from above."
}], "Text",
 CellChangeTimes->{{3.7810205032712607`*^9, 3.781020593251562*^9}, {
   3.7810206484301987`*^9, 3.781020673809305*^9}, 3.7810207093054037`*^9},
 FontSize->24,ExpressionUUID->"2385a4f9-4c7d-433c-bb88-552bb12792a0"]
}, Closed]]
}, Open  ]],

