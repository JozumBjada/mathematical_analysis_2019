Cell[TextData[Cell[BoxData[
 FormBox[
  RowBox[{
   RowBox[{
    FractionBox["n", 
     SuperscriptBox[
      RowBox[{"(", 
       RowBox[{
        RowBox[{"2", "n"}], "+", "1"}], ")"}], "3"]], "\[LessEqual]", 
    FractionBox[
     RowBox[{
      RowBox[{"2", "n"}], "+", "1"}], 
     SuperscriptBox[
      RowBox[{"(", 
       RowBox[{
        RowBox[{"2", "n"}], "+", "1"}], ")"}], "3"]]}], "=", 
   RowBox[{
    FractionBox["1", 
     RowBox[{
      RowBox[{"(", 
       RowBox[{
        RowBox[{"2", "n"}], "+", "1"}], ")"}], 
      RowBox[{"(", 
       RowBox[{
        RowBox[{"2", "n"}], "+", "1"}], ")"}]}]], "\[LessEqual]", 
    FractionBox["1", 
     RowBox[{
      RowBox[{"(", 
       RowBox[{"n", "+", "1"}], ")"}], 
      RowBox[{"(", 
       RowBox[{"n", "+", "1"}], ")"}]}]], "\[LessEqual]", 
    FractionBox["1", 
     RowBox[{"n", "(", 
      RowBox[{"n", "+", "1"}], ")"}]]}]}], TraditionalForm]],
 FormatType->
  "TraditionalForm",ExpressionUUID->"e9930b96-fe4c-4d8d-828e-b4d457700761"]], \
"Text",
 CellChangeTimes->{{3.782192880071587*^9, 3.782192939624364*^9}},
 FontSize->36,ExpressionUUID->"58e2eb4b-6b01-4e7c-8f3b-87aa626d7ebe"],

Cell[TextData[{
 "We know that ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    UnderoverscriptBox["\[Sum]", 
     RowBox[{"n", "=", "1"}], "\[Infinity]"], 
    FractionBox["1", 
     RowBox[{"n", "(", 
      RowBox[{"n", "+", "1"}], ")"}]]}], TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "7655bfe2-8436-46f3-ad52-c03501a6a457"],
 " converges (see the beginning of page 44 in Skriptum), so ",
 Cell[BoxData[
  FormBox[
   UnderoverscriptBox["\[Sum]", 
    RowBox[{"n", "=", "1"}], "\[Infinity]"], TraditionalForm]],ExpressionUUID->
  "2889853c-efc4-4783-ac51-199f1d2fe28d"],
 Cell[BoxData[
  FormBox[
   FractionBox["n", 
    SuperscriptBox[
     RowBox[{"(", 
      RowBox[{
       RowBox[{"2", "n"}], "+", "1"}], ")"}], "3"]], TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "45c02f04-301c-4014-8645-d6e55efb5685"],
 "also ",
