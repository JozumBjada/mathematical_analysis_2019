Cell[CellGroupData[{

Cell["Solution", "Chapter",
 CellChangeTimes->{{3.781016572580119*^9, 
  3.781016573215418*^9}},ExpressionUUID->"fe42817d-77ef-4080-add0-\
0722ed48c35a"],

Cell["Quotientenkriterium:", "Text",
 CellChangeTimes->{{3.7812618183163867`*^9, 3.7812618613708105`*^9}, {
   3.781262080583765*^9, 3.7812620966825104`*^9}, {3.781262159098371*^9, 
   3.7812622004473066`*^9}, {3.782191971276503*^9, 3.7821919820955763`*^9}, 
   3.782194779314956*^9},
 FontSize->36,ExpressionUUID->"dceb4130-5147-450b-a0fa-f1c965d2ddc9"],

Cell[TextData[Cell[BoxData[
 FormBox[
  RowBox[{
   RowBox[{
    SubscriptBox["\[Limit]", 
     RowBox[{"n", "\[Rule]", "\[Infinity]"}]], 
    FractionBox[
     TemplateBox[{SubscriptBox["a", 
        RowBox[{"n", "+", "1"}]]},
      "Abs"], 
     TemplateBox[{SubscriptBox["a", "n"]},
      "Abs"]]}], "=", 
   RowBox[{
    RowBox[{
     SubscriptBox["\[Limit]", 
      RowBox[{"n", "\[Rule]", "\[Infinity]"}]], 
     FractionBox[
      TemplateBox[{RowBox[{
          FractionBox[
           SuperscriptBox[
            RowBox[{"(", 
              RowBox[{"-", "1"}], ")"}], 
            RowBox[{"n", "+", "2"}]], 
           RowBox[{"n", "+", "1"}]], 
          SuperscriptBox["x", 
           RowBox[{"n", "+", "1"}]]}]},
       "Abs"], 
      TemplateBox[{RowBox[{
          FractionBox[
           SuperscriptBox[
            RowBox[{"(", 
              RowBox[{"-", "1"}], ")"}], 
            RowBox[{"n", "+", "1"}]], "n"], 
          SuperscriptBox["x", "n"]}]},
       "Abs"]]}], "=", 
    RowBox[{
     RowBox[{
      TemplateBox[{"x"},
       "Abs"], "\[CenterDot]", 
      RowBox[{
       SubscriptBox["\[Limit]", 
        RowBox[{"n", "\[Rule]", "\[Infinity]"}]], 
       FractionBox["n", 
        RowBox[{"n", "+", "1"}]]}]}], "=", 
     TemplateBox[{"x"},
      "Abs"]}]}]}], TraditionalForm]],
 FormatType->
  "TraditionalForm",ExpressionUUID->"bd06a312-570c-4538-acbe-8fa0253a9c2f"]], \
"Text",
 CellChangeTimes->{{3.7821948069540596`*^9, 3.7821948836170864`*^9}},
 FontSize->36,ExpressionUUID->"46471dc2-b6e2-4843-90ed-9686277adcc5"],

Cell[TextData[{
 "So for ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    TemplateBox[{"x"},
     "Abs"], "<", "1"}], TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "02a9d740-22cc-412a-b9f3-f0bc3801201f"],
 " converges absolutely, for ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    TemplateBox[{"x"},
     "Abs"], ">", "1"}], TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "8c5feabd-4950-4a74-bcb2-4771f584bf99"],
 " diverges."
}], "Text",
 CellChangeTimes->{{3.7821949059673643`*^9, 3.7821949365086737`*^9}},
 FontSize->36,ExpressionUUID->"ca8a2801-c834-4759-8569-d653784dbdc8"],

Cell[TextData[{
 "For ",
 Cell[BoxData[
  FormBox[
   RowBox[{"x", "=", "1"}], TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "1bab82b6-81b9-4a84-ba1a-d9aea85b0921"],
 " we get:  ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     UnderoverscriptBox["\[Sum]", 
      RowBox[{"n", "=", "1"}], "\[Infinity]"], 
     RowBox[{
      FractionBox[
       SuperscriptBox[
        RowBox[{"(", 
         RowBox[{"-", "1"}], ")"}], 
        RowBox[{"n", "+", "1"}]], "n"], 
      SuperscriptBox["1", "n"]}]}], "=", 
    RowBox[{"-", 
     RowBox[{
      UnderoverscriptBox["\[Sum]", 
       RowBox[{"n", "=", "1"}], "\[Infinity]"], 
      RowBox[{
       SuperscriptBox[
        RowBox[{"(", 
         RowBox[{"-", "1"}], ")"}], "n"], 
       FractionBox["1", "n"]}]}]}]}], TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "a26a8a8a-8657-4f71-853f-687befe29384"],
 ". Using the Leibnitz Kriterium, where ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["a", "n"], ":=", 
    FractionBox["1", "n"]}], TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "6ed8ff09-0d54-4a96-8e99-f34b9dc137da"],
 ", we can easily see that the Reihe converges. The Reihe does not converge \
absolutely though, as  ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     UnderoverscriptBox["\[Sum]", 
      RowBox[{"n", "=", "1"}], "\[Infinity]"], 
     FormBox[
      TemplateBox[{FractionBox[
         RowBox[{
           SuperscriptBox[
            RowBox[{"(", 
              RowBox[{"-", "1"}], ")"}], 
            RowBox[{"n", "+", "1"}]], " ", 
           SuperscriptBox["1", "n"]}], "n"]},
       "Abs"],
      TraditionalForm]}], "=", 
    RowBox[{
     UnderoverscriptBox["\[Sum]", 
      RowBox[{"n", "=", "1"}], "\[Infinity]"], 
     FractionBox["1", "n"]}]}], TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "989ca19e-4c3b-4233-a121-f612ade92454"],
 ". It is the harmonische Reihe, which diverges."
}], "Text",
 CellChangeTimes->{{3.782194945366991*^9, 3.7821950270525913`*^9}, {
   3.7821950971746197`*^9, 3.7821951720254936`*^9}, 3.782195277326953*^9, {
   3.782197425652144*^9, 3.782197430066306*^9}},
 FontSize->36,ExpressionUUID->"c8176dba-9621-46e0-b474-2d8ab2cb5a35"],

Cell[TextData[{
 "For ",
 Cell[BoxData[
  FormBox[
   RowBox[{"x", "=", 
    RowBox[{"-", "1"}]}], TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "fc6889f3-bf31-4395-9f52-344f8104c84d"],
 " we get:  ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     UnderoverscriptBox["\[Sum]", 
      RowBox[{"n", "=", "1"}], "\[Infinity]"], 
     RowBox[{
      FractionBox[
       SuperscriptBox[
        RowBox[{"(", 
         RowBox[{"-", "1"}], ")"}], 
        RowBox[{"n", "+", "1"}]], "n"], 
      SuperscriptBox[
       RowBox[{"(", 
        RowBox[{"-", "1"}], ")"}], "n"]}]}], "=", 
    RowBox[{"-", 
     RowBox[{
      UnderoverscriptBox["\[Sum]", 
       RowBox[{"n", "=", "1"}], "\[Infinity]"], 
      FractionBox["1", "n"]}]}]}], TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "d37e80cd-06f3-49ee-a4bc-16e354d6fe9e"],
 ", i.e. the harmonische Reihe, which diverges."
}], "Text",
 CellChangeTimes->{{3.782195028772992*^9, 3.7821950661869564`*^9}},
 FontSize->36,ExpressionUUID->"45ab6bce-6a05-4f64-823b-5b0f0ea83f03"],

Cell["To conclude:", "Text",
 CellChangeTimes->{{3.7821951987829533`*^9, 3.782195200476425*^9}},
 FontSize->36,ExpressionUUID->"a3fe8054-4452-4a05-9fd2-6718016f160c"],

Cell[TextData[{
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"-", "\[Infinity]"}], "<", "x", "\[LessEqual]", 
    RowBox[{"-", "1"}]}], TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "484a2ea8-0448-48f4-b5b5-63371c52cdde"],
 ": divergent\n",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"-", "1"}], "<", "x", "<", "1"}], TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "5e8e9ad3-30ed-4376-95a3-4be25fea4e5c"],
 ": absolutely convergent\n",
 Cell[BoxData[
  FormBox[
   RowBox[{"x", "=", "1"}], TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "86eb4d58-b625-4764-8e7d-fa9ca83d3d19"],
 ": convergent (not absolutely)\n",
 Cell[BoxData[
  FormBox[
   RowBox[{"1", "<", "x", "<", "\[Infinity]"}], TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "44c25cbb-9376-4e27-b384-9cc6d4646563"],
 ": divergent"
}], "Text",
 CellChangeTimes->{{3.782195202338449*^9, 3.7821952670763597`*^9}},
 FontSize->36,
 FontWeight->"Bold",ExpressionUUID->"8daf897a-ac3d-4181-abf0-5ecf4daaed3a"]
}, Closed]]
}, Closed]],

Cell[CellGroupData[{

Cell["5) Cauchy-Produkt", "Title",
 CellChangeTimes->{{3.7810164232069955`*^9, 3.781016431256077*^9}, {
  3.781254892344835*^9, 3.781254910886995*^9}, {3.782195475466325*^9, 
  3.782195479476602*^9}},ExpressionUUID->"a5d762b4-2c7e-4aa7-8000-\
fb3c090635c7"],

Cell[TextData[Cell[BoxData[
 FormBox[
  RowBox[{
   FractionBox["1", 
    RowBox[{"1", "-", "x"}]], "=", 
