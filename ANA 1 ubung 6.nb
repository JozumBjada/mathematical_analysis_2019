      TemplateBox[{SubscriptBox["a", "n"]},
       "Abs"]]}], "=", 
    RowBox[{
     RowBox[{
      SubscriptBox["\[Limit]", 
       RowBox[{"n", "\[Rule]", "\[Infinity]"}]], 
      FractionBox[
       TemplateBox[{FractionBox[
          SuperscriptBox["x", 
           RowBox[{
             RowBox[{"3", "n"}], "+", "3"}]], 
          RowBox[{
            RowBox[{"(", 
              RowBox[{
                RowBox[{"2", "n"}], "+", "2"}], ")"}], "!"}]]},
        "Abs"], 
       TemplateBox[{FractionBox[
          SuperscriptBox["x", 
           RowBox[{"3", "n"}]], 
          RowBox[{
            RowBox[{"(", 
              RowBox[{"2", "n"}], ")"}], "!"}]]},
        "Abs"]]}], "=", 
     RowBox[{
      RowBox[{
       TemplateBox[{SuperscriptBox["x", "3"]},
        "Abs"], "\[CenterDot]", 
       RowBox[{
        SubscriptBox["\[Limit]", 
         RowBox[{"n", "\[Rule]", "\[Infinity]"}]], 
        FractionBox[
         RowBox[{
          RowBox[{"(", 
           RowBox[{"2", "n"}], ")"}], "!"}], 
         RowBox[{
          RowBox[{"(", 
           RowBox[{
            RowBox[{"2", "n"}], "+", "2"}], ")"}], "!"}]]}]}], "=", 
      RowBox[{
       RowBox[{
        TemplateBox[{SuperscriptBox["x", "3"]},
         "Abs"], "\[CenterDot]", 
        RowBox[{
         SubscriptBox["\[Limit]", 
          RowBox[{"n", "\[Rule]", "\[Infinity]"}]], 
         FractionBox["1", 
          RowBox[{
           RowBox[{"(", 
            RowBox[{
             RowBox[{"2", "n"}], "+", "2"}], ")"}], 
           RowBox[{"(", 
            RowBox[{
             RowBox[{"2", "n"}], "+", "1"}], ")"}]}]]}]}], "=", 
       RowBox[{
        RowBox[{
         TemplateBox[{SuperscriptBox["x", "3"]},
          "Abs"], "\[CenterDot]", 
         RowBox[{
          SubscriptBox["\[Limit]", 
           RowBox[{"n", "\[Rule]", "\[Infinity]"}]], 
          RowBox[{
           FractionBox["1", 
            RowBox[{
             RowBox[{"(", 
              RowBox[{"2", "+", 
               FractionBox["2", "n"]}], ")"}], 
             RowBox[{"(", 
              RowBox[{"2", "+", 
               FractionBox["1", "n"]}], ")"}]}]], 
           FractionBox["1", 
            SuperscriptBox["n", "2"]]}]}]}], "=", 
        RowBox[{"0", "=", "\[Theta]"}]}]}]}]}]}], TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "a6a989f6-7ca0-4afb-a0c7-c21a4389164d"]
}], "Text",
