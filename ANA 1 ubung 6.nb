Cell[TextData[Cell[BoxData[
 FormBox[
  RowBox[{
   SubscriptBox["a", "n"], ":=", 
   FractionBox["n", 
    SuperscriptBox[
     RowBox[{"(", 
      RowBox[{
       RowBox[{"2", "n"}], "+", "1"}], ")"}], "3"]]}], 
  TraditionalForm]],ExpressionUUID->"5c93f304-7004-4322-9d9f-9adb63ac976f"]], \
