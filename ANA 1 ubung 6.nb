Cell[TextData[Cell[BoxData[
 FormBox[
  RowBox[{
   SubscriptBox["a", "n"], ":=", 
   FractionBox["1", 
    SqrtBox["n"]]}], TraditionalForm]],
 FormatType->
  "TraditionalForm",ExpressionUUID->"84596fae-d56b-48c5-9667-545e2d7581e1"]], \
