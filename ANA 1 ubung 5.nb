Cell[CellGroupData[{

Cell["Bounded", "Section",
 CellChangeTimes->{{3.7812560352733326`*^9, 
  3.7812560360497537`*^9}},ExpressionUUID->"d1b1c2a3-09b5-4177-97b2-\
676f3cb00719"],

Cell[TextData[{
 "First, we show by mathematical induction that ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["a", "n"], "<", 
    FractionBox[
     RowBox[{"1", "+", 
      SqrtBox["5"]}], "2"]}], TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "1655e2c4-648b-4c49-8439-a9b39652f118"],
 ":"
}], "Text",
 CellChangeTimes->{{3.781017460671653*^9, 3.7810175391231174`*^9}, {
  3.78101759274957*^9, 3.7810176004385896`*^9}, {3.781255297453834*^9, 
  3.781255344333031*^9}, {3.7812555594274797`*^9, 3.7812555594274797`*^9}},
 FontSize->24,ExpressionUUID->"9b8e0dfa-1612-426c-a396-ede1990df632"],

Cell[TextData[{
 "initial step for ",
 Cell[BoxData[
  FormBox[
   RowBox[{"n", "=", "0"}], TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "d4d28531-c25b-4707-9fe3-e889dc045e98"],
 ":"
}], "Item",
 CellChangeTimes->{{3.781255306601053*^9, 3.781255315291052*^9}, 
   3.781255350830248*^9},
 FontSize->24,
 FontWeight->"Bold",ExpressionUUID->"4e401daf-e670-40b6-999e-fcc40ba20e3e"],

Cell[TextData[Cell[BoxData[
 FormBox[
  RowBox[{
   SubscriptBox["a", "0"], "=", 
   RowBox[{"1", "<", 
    FractionBox[
     RowBox[{"1", "+", 
      SqrtBox["5"]}], "2"], "\[TildeTilde]", "1.618033988749895`"}]}], 
  TraditionalForm]],
 EmphasizeSyntaxErrors->
  True,ExpressionUUID->"0d78b4c3-8ac2-4f4b-bb3a-eedf735333c2"]], "Text",
 CellChangeTimes->{{3.7812553747245502`*^9, 3.7812553905444603`*^9}, {
  3.7812555264224377`*^9, 3.7812555457380085`*^9}},
 FontSize->24,ExpressionUUID->"5b91e908-4b90-4972-b192-a5a91e01f860"],

Cell[TextData[{
 "assume validity for ",
 Cell[BoxData[
  FormBox["n", TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "6af4cab2-7bb7-4eed-a478-eeebf93c16ba"],
 " to prove the same for ",
 Cell[BoxData[
  FormBox[
   RowBox[{"n", "+", "1"}], TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "7948cd13-f289-4d2f-b766-6b64160d36a1"],
 ":"
}], "Item",
 CellChangeTimes->{{3.781255306601053*^9, 3.781255315291052*^9}, 
   3.781255350830248*^9, {3.7812553995384254`*^9, 3.78125541778504*^9}},
 FontSize->24,
 FontWeight->"Bold",ExpressionUUID->"4279c896-e5ef-4131-aeef-ea8c94642ea7"],

Cell[TextData[Cell[BoxData[
 FormBox[
  RowBox[{
   SubsuperscriptBox["a", 
    RowBox[{"n", "+", "1"}], "2"], "=", 
   RowBox[{
    RowBox[{
     RowBox[{"1", "+", 
      SubscriptBox["a", "n"]}], "<", 
     RowBox[{"1", "+", 
      FractionBox[
       RowBox[{"1", "+", 
        SqrtBox["5"]}], "2"]}]}], "=", 
    RowBox[{
     FractionBox[
      RowBox[{"3", "+", 
       SqrtBox["5"]}], "2"], "=", 
     RowBox[{
      SuperscriptBox[
       RowBox[{"(", 
        FractionBox[
         RowBox[{"1", "+", 
          SqrtBox["5"]}], "2"], ")"}], "2"], " ", "\[Implies]", " ", 
      RowBox[{
       RowBox[{
        SubsuperscriptBox["a", 
         RowBox[{"n", "+", "1"}], "2"], "<", 
        SuperscriptBox[
         RowBox[{"(", 
          FractionBox[
           RowBox[{"1", "+", 
            SqrtBox["5"]}], "2"], ")"}], "2"]}], "\[Implies]", " ", 
       RowBox[{
        SubscriptBox["a", 
         RowBox[{"n", "+", "1"}]], "<", 
        FractionBox[
         RowBox[{"1", "+", 
          SqrtBox["5"]}], "2"]}]}]}]}]}]}], 
  TraditionalForm]],ExpressionUUID->"08a3fcb6-95a7-4be5-b841-a3375662290c"]], \
"Text",
 CellChangeTimes->{{3.7812553747245502`*^9, 3.7812553905444603`*^9}, {
  3.7812554295952787`*^9, 3.781255514577901*^9}},
 FontSize->24,ExpressionUUID->"7f971419-8efc-4a94-a1b5-8f1e1af9c65f"]
}, Closed]],

Cell[CellGroupData[{

Cell["Increasing", "Section",
 CellChangeTimes->{{3.7812560417536063`*^9, 
  3.781256043657424*^9}},ExpressionUUID->"e8b7873b-02ac-472c-b7fe-\
42d01b29d2a6"],

Cell[TextData[{
 "Secondly, we show that ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["a", 
     RowBox[{"n", "+", "1"}]], ">", 
    SubscriptBox["a", "n"]}], TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "4e837640-2971-440b-ab52-8875cf106219"],
 ":"
}], "Text",
 CellChangeTimes->{{3.781017460671653*^9, 3.7810175391231174`*^9}, {
  3.78101759274957*^9, 3.7810176004385896`*^9}, {3.781255297453834*^9, 
  3.781255344333031*^9}, {3.7812555594274797`*^9, 3.7812555594274797`*^9}, {
  3.781255601170454*^9, 3.7812556196211843`*^9}},
 FontSize->24,ExpressionUUID->"e9e64236-1190-4b8b-88c8-2b4cc95cf2dc"],

Cell[TextData[{
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     RowBox[{
      SubscriptBox["a", "n"], "<", 
      SubscriptBox["a", 
       RowBox[{"n", "+", "1"}]]}], "   ", "\[DoubleLeftRightArrow]", "  ", 
     RowBox[{
      SubsuperscriptBox["a", "n", "2"], "<", " ", 
      SubsuperscriptBox["a", 
       RowBox[{"n", "+", "1"}], "2"]}]}], "=", 
    RowBox[{"1", "+", 
     SubscriptBox["a", "n"]}]}], TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "6101afd3-171f-429b-983b-e729679a26cf"],
 "     and so we want to check that:  ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     SubsuperscriptBox["a", "n", "2"], "-", 
     SubscriptBox["a", "n"], "-", "1"}], "<", " ", "0"}], TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "f310d8e4-8f5a-49d3-9991-8169fad0335d"]
}], "Text",
 CellChangeTimes->{{3.781255628198058*^9, 3.7812557289198084`*^9}},
 FontSize->24,ExpressionUUID->"5a638dc2-777f-429b-83f3-e98ab3c7b7bc"],

Cell[TextData[{
 "Solutions to ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     SubsuperscriptBox["a", "n", "2"], "-", 
     SubscriptBox["a", "n"], "-", "1"}], "<", " ", "0"}], TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "ea654d91-b61c-41a0-a0d8-b2227d89ae91"],
 " are ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox[
     RowBox[{"(", 
      SubscriptBox["a", "n"], ")"}], 
     RowBox[{"1", ",", "2"}]], "=", 
    FractionBox[
     RowBox[{"1", "\[PlusMinus]", 
      SqrtBox["5"]}], "2"]}], TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "a6648cc7-479e-49c4-8e32-7a7e3ae55ffb"],
 ". From the plot of the function ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SuperscriptBox["x", "2"], "-", "x", "-", "1"}], TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "5b38a672-8473-442d-970d-709695d58293"],
 " and from the property ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["a", "n"], "<", 
    FractionBox[
     RowBox[{"1", "+", 
      SqrtBox["5"]}], "2"]}], TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "005c519e-2237-4f14-bbfb-1cfcba30b6cd"],
 " it is clear that indeed ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     SubsuperscriptBox["a", "n", "2"], "-", 
     SubscriptBox["a", "n"], "-", "1"}], "<", " ", "0"}], TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "8bb9d4bd-b6af-4fa4-ac9b-b4dc12e18098"],
 " and thus ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["a", 
     RowBox[{"n", "+", "1"}]], ">", 
    SubscriptBox["a", "n"]}], TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "c85ac1e8-bd37-485f-ad58-9c40ea571613"],
 "."
}], "Text",
 CellChangeTimes->{{3.7812557486630216`*^9, 3.781255775244055*^9}, {
  3.781255967056165*^9, 3.781256021675568*^9}},
 FontSize->24,ExpressionUUID->"26c15f46-8a80-4104-bc9b-e1b68ab45175"],

