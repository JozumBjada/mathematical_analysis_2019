  FormBox[
   RowBox[{
    RowBox[{"sin", "(", 
     RowBox[{"k", " ", "x"}], ")"}], "=", 
    RowBox[{
     RowBox[{"sin", "(", 
      RowBox[{"k", " ", 
       FractionBox["\[Pi]", "2"], 
       FractionBox["1", "k"]}], ")"}], "=", 
     RowBox[{
      RowBox[{"sin", "(", 
       FractionBox["\[Pi]", "2"], ")"}], "=", "1"}]}]}], TraditionalForm]],
  ExpressionUUID->"1228e248-5cb7-4d4e-84b0-91153a4fc9bc"],
 ". We have:"
}], "Text",
