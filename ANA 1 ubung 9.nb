Cell["We get:", "Text",
 CellChangeTimes->{{3.784288251240306*^9, 3.7842882541415434`*^9}},
 FontSize->36,ExpressionUUID->"1272ac22-6fc9-4047-95d7-5d9c7ef83b83"],

Cell[TextData[Cell[BoxData[
 FormBox[
  RowBox[{
   RowBox[{
    RowBox[{"\[Psi]", "'"}], 
    RowBox[{"(", "x", ")"}]}], "=", 
   RowBox[{
    RowBox[{
     RowBox[{"(", 
      SuperscriptBox["\[ExponentialE]", 
       RowBox[{"-", 
        FractionBox[
         SuperscriptBox["x", "2"], 
         RowBox[{"2", 
          SuperscriptBox["a", "2"]}]]}]], ")"}], "'"}], "=", 
    RowBox[{
     RowBox[{
      RowBox[{
       SuperscriptBox["\[ExponentialE]", 
        RowBox[{"-", 
         FractionBox[
          SuperscriptBox["x", "2"], 
          RowBox[{"2", 
           SuperscriptBox["a", "2"]}]]}]], "\[CenterDot]", 
       RowBox[{"(", 
        RowBox[{"-", 
         FractionBox["1", 
          RowBox[{"2", " ", 
           SuperscriptBox["a", "2"]}]]}], ")"}]}], "2", "x"}], "=", 
     RowBox[{
      RowBox[{
       RowBox[{"-", 
        FractionBox["x", 
         SuperscriptBox["a", "2"]]}], 
       SuperscriptBox["\[ExponentialE]", 
        RowBox[{"-", 
         FractionBox[
          SuperscriptBox["x", "2"], 
          RowBox[{"2", 
           SuperscriptBox["a", "2"]}]]}]]}], "=", 
      RowBox[{
       RowBox[{"-", 
        FractionBox["x", 
         SuperscriptBox["a", "2"]]}], 
       RowBox[{"\[Psi]", "(", "x", ")"}]}]}]}]}]}], 
  TraditionalForm]],ExpressionUUID->"0318b5f1-e67f-4dae-b5ec-e7fdb584e493"]], \
"Text",
 CellChangeTimes->{{3.7842873975060515`*^9, 3.7842874376367116`*^9}, {
  3.784287838159248*^9, 3.7842878603928013`*^9}, {3.7842880123792276`*^9, 
  3.7842880200696583`*^9}},
 FontSize->36,ExpressionUUID->"13961f5c-f156-494e-8a93-d822b528e604"],

Cell["\<\
where we used the formula for Ableitung of composite functions and formulas \
for Ableitung of the exponential function and monomial functions. Similarly:\
\>", "Text",
 CellChangeTimes->{{3.784287447310835*^9, 3.784287506592269*^9}, {
  3.7842879925313225`*^9, 3.784288007530204*^9}, {3.7842884127782125`*^9, 
  3.784288412884926*^9}},
 FontSize->36,ExpressionUUID->"132fd88a-a1eb-45a5-a0fb-4f6cd6b9fce3"],

Cell[TextData[Cell[BoxData[
 FormBox[
  RowBox[{
   RowBox[{
    RowBox[{"\[Psi]", "''"}], 
    RowBox[{"(", "x", ")"}]}], "=", 
   RowBox[{
    RowBox[{
     RowBox[{"(", 
      RowBox[{
       RowBox[{"-", 
        FractionBox["x", 
         SuperscriptBox["a", "2"]]}], 
       RowBox[{"\[Psi]", "(", "x", ")"}]}], ")"}], "'"}], "=", 
    RowBox[{
     RowBox[{
      RowBox[{
       RowBox[{"(", 
        RowBox[{"-", 
         FractionBox["1", 
          SuperscriptBox["a", "2"]]}], ")"}], 
       RowBox[{"\[Psi]", "(", "x", ")"}]}], "+", 
      RowBox[{
       RowBox[{"(", 
        RowBox[{"-", 
         FractionBox["x", 
          SuperscriptBox["a", "2"]]}], ")"}], 
       RowBox[{"\[Psi]", "'"}], 
       RowBox[{"(", "x", ")"}]}]}], "=", 
     RowBox[{
      RowBox[{
       RowBox[{
        RowBox[{"(", 
         RowBox[{"-", 
          FractionBox["1", 
           SuperscriptBox["a", "2"]]}], ")"}], 
        RowBox[{"\[Psi]", "(", "x", ")"}]}], "+", 
       RowBox[{
        RowBox[{"(", 
         RowBox[{"-", 
          FractionBox["x", 
           SuperscriptBox["a", "2"]]}], ")"}], 
        RowBox[{"(", 
         RowBox[{
          RowBox[{"-", 
           FractionBox["x", 
            SuperscriptBox["a", "2"]]}], 
          RowBox[{"\[Psi]", "(", "x", ")"}]}], ")"}]}]}], "=", 
      RowBox[{
       RowBox[{
        RowBox[{"(", 
         RowBox[{"-", 
          FractionBox["1", 
           SuperscriptBox["a", "2"]]}], ")"}], 
        RowBox[{"\[Psi]", "(", "x", ")"}], 
        RowBox[{"(", 
         RowBox[{"1", "-", 
          FractionBox[
           SuperscriptBox["x", "2"], 
           SuperscriptBox["a", "2"]]}], ")"}]}], "=", 
       RowBox[{
        RowBox[{
         RowBox[{"(", 
          FractionBox[
           RowBox[{
            SuperscriptBox["x", "2"], "-", 
            SuperscriptBox["a", "2"]}], 
           SuperscriptBox["a", "4"]], ")"}], 
         RowBox[{"\[Psi]", "(", "x", ")"}]}], "=", 
        RowBox[{
         RowBox[{"(", 
          FractionBox[
           RowBox[{
            SuperscriptBox["x", "2"], "-", 
            SuperscriptBox["a", "2"]}], 
           SuperscriptBox["a", "4"]], ")"}], 
         SuperscriptBox["\[ExponentialE]", 
          RowBox[{"-", 
           FractionBox[
            SuperscriptBox["x", "2"], 
            RowBox[{"2", 
             SuperscriptBox["a", "2"]}]]}]]}]}]}]}]}]}]}], 
  TraditionalForm]],ExpressionUUID->"f33b3711-2b01-45d9-a6e2-ce9b1b370488"]], \
"Text",
 CellChangeTimes->{{3.7842873975060515`*^9, 3.7842874376367116`*^9}, {
  3.784287838159248*^9, 3.7842878603928013`*^9}, {3.7842880123792276`*^9, 
  3.7842881904200277`*^9}},
 FontSize->36,ExpressionUUID->"2c40d854-4c39-445b-8688-a698a5e1fa8e"],

